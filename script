(do
	(print (+ 1 2 3 4) "=" 10)
	(print (- 1 2 3) "=" (- 4))
	(print (* 3 4 2) "=" 24)
	(print (/ 4 2 2) "=" 1)
	(print (and true true false) "=" false)
	(print (and true true true) "=" true)
	(print (or true false false) "=" true)
	(print (or false false false) "=" false)
	(print (+ "a" "b") "= ab")
)
